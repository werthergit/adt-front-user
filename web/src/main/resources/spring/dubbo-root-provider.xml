<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">


    &lt;!&ndash; 应用信息,用于计算依赖关系 &ndash;&gt;
    <dubbo:application name="${dubbo.service.name}" owner="Telematcis" organization="adt123"/>

    &lt;!&ndash; 使用zookeeper注册中心 &ndash;&gt;
    <dubbo:registry protocol="zookeeper" address="${zk.server}" timeout="30000"/>

    &lt;!&ndash;uncomment this if you want to test dubbo's monitor&ndash;&gt;
    <dubbo:monitor protocol="registry"/>

    &lt;!&ndash; 使用dubbo协议 &ndash;&gt;
    <dubbo:protocol name="rest" port="${dubbo.service.port}" serialization="kryo" server="tomcat"
                    optimizer="com.adt.front.user.rpc.protocol.SerializationOptimizerImpl"
                    extension="com.adt.framework.monitor.cat.dubbospi.DubboCatCrossFilter"
    />

    &lt;!&ndash; 设定dubbo默认参数 &ndash;&gt;
    <dubbo:provider timeout="2000" retries="1" />
    <dubbo:consumer timeout="2000" retries="1" />

    &lt;!&ndash;PubSub Sub注入&ndash;&gt;
    <import resource="classpath*:context/rest-api-*.xml"/>

</beans>-->
